[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=chancen
Group=www-data
WorkingDirectory=/home/chancen/chancen-services-portal
ExecStart=/home/chancen/chancen-services-portal/.env/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/chancen/chancen-services-portal/csp.sock csp.wsgi:application

[Install]
WantedBy=multi-user.target